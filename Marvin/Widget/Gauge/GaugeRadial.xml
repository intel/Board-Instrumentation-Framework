<Widget Type = "SteelGaugeRadial">
    <Style>GaugeRadial.css</Style>
    <MinValue>0</MinValue>
    <MaxValue>100</MaxValue>
    <UnitText>Units</UnitText>
    <Decimals>2</Decimals>
    <!-- NOTE, angles are currently ignored, bug in the open source package -->
    <DialStartAngle>330</DialStartAngle>
    <DialRangeAngle>300</DialRangeAngle>
    <MajorTicksSpace>10</MajorTicksSpace>
    <MinorTicksSpace>5</MinorTicksSpace>
    <TickLableOrientation>Horizontal</TickLableOrientation>
    <EnhancedRateText>True</EnhancedRateText>
	<MaxSteppedRange>10,100,1000</MaxSteppedRange>
	<MinSteppedRange>0,-10,-100</MinSteppedRange>
</Widget>
